<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tom.fastapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConsumerActivity"></activity>
        <activity
            android:name=".PokerActivity"
            android:label="@string/title_activity_poker"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MapActivity" />
        <activity android:name=".RequestActivity" />
        <activity android:name=".BasicActivity" />
        <activity android:name=".WorkActivity" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".workerActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>